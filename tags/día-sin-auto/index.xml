tById(&#34;caption&#34;);&#xA;        if (n === slides.length) {&#xA;            slideIndex = 0&#xA;        } &#xA;        if (n &lt; 0) {&#xA;            slideIndex = slides.length - 1&#xA;        }&#xA;        for (i = 0; i &lt; slides.length; i++) {&#xA;            slides[i].style.display = &#34;none&#34;;&#xA;        }&#xA;        slides[slideIndex].style.display = &#34;block&#34;;&#xA;    }&#xA;&lt;/script&gt;&#xA;&lt;br&gt;&lt;/p&gt;</description>
